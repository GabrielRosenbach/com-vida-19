unit DataModule;

interface

uses
  System.SysUtils, System.Classes, REST.Types, REST.Client,
  REST.Authenticator.Basic, Data.Bind.Components, Data.Bind.ObjectScope;

type
  TDM = class(TDataModule)
    RESTClient1: TRESTClient;
    RESTRequestValidarLogin: TRESTRequest;
    RESTResponse1: TRESTResponse;
    HTTPBasicAuthenticator1: THTTPBasicAuthenticator;
    RESTRequestCadastro: TRESTRequest;
  private
    { Private declarations }
  public
    { Public declarations }
   function TDataModule.Cadastro(nome: string;login: string; senha : string; email :string;tel : string;codtipusu : integer;out erro:string):boolean ;

  end;

var
  DM: TDM;

implementation

{%CLASSGROUP 'FMX.Controls.TControl'}

{$R *.dfm}
     RESTRequestCadastro.Params.Clear;
     RESTRequestCadastro.AddParameter('nome',edtNome.Text,TRESTRequestParameterKind.pkGETorPOST);
     RESTRequestCadastro.AddParameter('login',edtlogin.Text,TRESTRequestParameterKind.pkGETorPOST);
     RESTRequestCadastro.AddParameter('email',edtEmail.Text,TRESTRequestParameterKind.pkGETorPOST);
     RESTRequestCadastro.AddParameter('tel',edtTel.Text,TRESTRequestParameterKind.pkGETorPOST);
     RESTRequestCadastro.AddParameter('senha',edtSenha.Text,TRESTRequestParameterKind.pkGETorPOST);
     RESTRequestCadastro.AddParameter('codtipusu',edit1.Text,TRESTRequestParameterKind.pkGETorPOST);
     RESTRequestCadastro.Execute;


end.
